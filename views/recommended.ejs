<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Recommanded Recipes</title>

    <script type="text/javascript">
        let recipes = <%- JSON.stringify(recipes) %>
    </script>

    <script type="text/javascript" src="/recommended.js"></script>

</head>
<body>
    <%- include("navbar.ejs") %>


    <div class="container">
        <% recipes.forEach((recipe) => { %>
            <h3><a href="/meals/<%= recipe._id %>"><%= recipe.title %></a></h3>
        <% }) %>
        
    </div>
</body>
</html>